{
  "compilerOptions": {
   "target": "es2019",
   "module": "commonjs",
   "lib": [
      "ES2020",
      "ES2017",
      "dom",
      "esnext.asynciterable",
    ],
    "strict": true,
    "outDir": "./dist",
    "esModuleInterop": true,
    "declaration": true,
    "sourceMap": false,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "typedocOptions": {
    "entryPoints": ["src/server.ts", "src/RequestHandler.ts", "src/utils/APIHandler.ts", "src/utils/Config.ts", "src/utils/DefaultListenner.ts", "src/utils/EventHandler.ts", "src/utils/util.ts"],
    "out": "docs"
  },
  "exclude": [
    "./test"
  ]
}
